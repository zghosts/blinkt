{
    "name": "zghosts/blinkt",
    "description": "A library for using the pimoroni blinkt on a Raspberry Pi",
    "keywords": ["gpio", "pi", "raspberry", "pin", "pins", "blinkt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zawadi Elstgeest",
            "email": "zawadi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zghosts\\Blinkt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zghosts\\Blinkt\\Test\\": "test/"
        }
    },
    "require": {
        "php": "^7.3||^8.0",
        "piphp/gpio": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "phpstan/phpstan": "^0.12.90",
        "symplify/easy-coding-standard": "^9.3",
        "phpspec/prophecy-phpunit": "^2.0",
        "jangregor/phpstan-prophecy": "^0.8.1"
    },
    "conflict": {
        "symplify/easy-coding-standard": "9.3.27"
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpunit"
        ]
    }
}
